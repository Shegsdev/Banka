{
  "name": "banka",
  "version": "1.0.0",
  "description": "A light-weight core banking application that powers banking operations.",
  "main": "server.js",
  "scripts": {
    "start": "babel-node server/server.js",
    "test": "npm run migration:seed && nyc --reporter=lcov --reporter=text mocha --require babel-register --timeout 40000 ./server/tests/*.js --exit",
    "dev": "babel-node build.js",
    "coveralls": "nyc report --reporter=text-lcov cat ./coverage/lcov.info | coveralls",
    "migration:create": "set DEBUG=express:migration & babel-node server/utils/createTables",
    "migration:drop": "set DEBUG=express:migration & babel-node server/utils/dropTables",
    "migration:seed": "set DEBUG=express:migration & babel-node server/database/seeders"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shegsdev/Banka.git"
  },
  "keywords": [
    "banking",
    "banking-application",
    "bank",
    "banking-solutions",
    "banking-web-application"
  ],
  "author": "Segun Akanbi (twitter.com/shegsdev)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Shegsdev/Banka/issues"
  },
  "homepage": "https://github.com/Shegsdev/Banka#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "mocha": "^6.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "5.0.0",
    "body-parser": "^1.18.3",
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.2",
    "validator": "^10.11.0"
  }
}
